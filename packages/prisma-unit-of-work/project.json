{
  "name": "prisma-unit-of-work",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma-unit-of-work/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/prisma-unit-of-work",
        "tsConfig": "packages/prisma-unit-of-work/tsconfig.lib.json",
        "packageJson": "packages/prisma-unit-of-work/package.json",
        "main": "packages/prisma-unit-of-work/src/index.ts",
        "assets": ["packages/prisma-unit-of-work/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs prisma-unit-of-work {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/prisma-unit-of-work/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/prisma-unit-of-work"],
      "options": {
        "jestConfig": "packages/prisma-unit-of-work/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
